<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_Signin" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.38.1" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.3.0">
  <bpmn:process id="user-signin-process" name="User Sign-In Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Sign-In Started">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="Task_VerifyCaptcha" name="Verify Captcha">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="captcha.verify" retries="3" />
        <zeebe:taskHeaders>
          <zeebe:header key="timeout" value="PT30S" />
          <zeebe:header key="maxAttempts" value="3" />
          <zeebe:header key="backoff" value="2000" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:boundaryEvent id="BoundaryEvent_CaptchaError" name="Captcha Failed" attachedToRef="Task_VerifyCaptcha">
      <bpmn:outgoing>Flow_CaptchaError</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_CaptchaError" errorRef="Error_CaptchaFailed" />
    </bpmn:boundaryEvent>
    <bpmn:exclusiveGateway id="Gateway_SigninMethod" name="SignIn Method?">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_Google</bpmn:outgoing>
      <bpmn:outgoing>Flow_LinkedIn</bpmn:outgoing>
      <bpmn:outgoing>Flow_InvalidMethod</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_SigninGoogle" name="Google OAuth SignIn">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="auth.signin.google" retries="3" />
        <zeebe:taskHeaders>
          <zeebe:header key="timeout" value="PT60S" />
          <zeebe:header key="backoff" value="5000" />
          <zeebe:header key="maxRetries" value="3" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Google</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:boundaryEvent id="BoundaryEvent_GoogleError" name="Google Auth Failed" attachedToRef="Task_SigninGoogle">
      <bpmn:outgoing>Flow_GoogleError</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_GoogleError" errorRef="Error_OAuthFailed" />
    </bpmn:boundaryEvent>
    <bpmn:serviceTask id="Task_SigninLinkedIn" name="LinkedIn OAuth SignIn">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="auth.signin.linkedin" retries="3" />
        <zeebe:taskHeaders>
          <zeebe:header key="timeout" value="PT60S" />
          <zeebe:header key="backoff" value="5000" />
          <zeebe:header key="maxRetries" value="3" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_LinkedIn</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:boundaryEvent id="BoundaryEvent_LinkedInError" name="LinkedIn Auth Failed" attachedToRef="Task_SigninLinkedIn">
      <bpmn:outgoing>Flow_LinkedInError</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_LinkedInError" errorRef="Error_OAuthFailed" />
    </bpmn:boundaryEvent>
    <bpmn:serviceTask id="Task_HandleCaptchaError" name="Handle Captcha Error">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="error.handler.captcha" retries="2" />
        <zeebe:taskHeaders>
          <zeebe:header key="timeout" value="PT30S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_CaptchaError</bpmn:incoming>
      <bpmn:outgoing>Flow_CaptchaErrorEnd</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_HandleOAuthError" name="Handle OAuth Error">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="error.handler.oauth" retries="2" />
        <zeebe:taskHeaders>
          <zeebe:header key="timeout" value="PT30S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_GoogleError</bpmn:incoming>
      <bpmn:incoming>Flow_LinkedInError</bpmn:incoming>
      <bpmn:outgoing>Flow_OAuthErrorEnd</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_HandleInvalidMethod" name="Handle Invalid Method">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="error.handler.invalid-method" retries="1" />
        <zeebe:taskHeaders>
          <zeebe:header key="timeout" value="PT20S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_InvalidMethod</bpmn:incoming>
      <bpmn:outgoing>Flow_InvalidMethodEnd</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_Join" name="Join Successful Auth">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="EndEvent_1" name="Sign-In Complete">
      <bpmn:incoming>Flow_5</bpmn:incoming>
    </bpmn:endEvent>
    <!-- FIXED: Regular end events without error definitions -->
    <bpmn:endEvent id="EndEvent_CaptchaError" name="Captcha Failed">
      <bpmn:incoming>Flow_CaptchaErrorEnd</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_OAuthError" name="OAuth Failed">
      <bpmn:incoming>Flow_OAuthErrorEnd</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_InvalidMethod" name="Invalid SignIn Method">
      <bpmn:incoming>Flow_InvalidMethodEnd</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_VerifyCaptcha" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_VerifyCaptcha" targetRef="Gateway_SigninMethod" />
    <bpmn:sequenceFlow id="Flow_Google" name="Google" sourceRef="Gateway_SigninMethod" targetRef="Task_SigninGoogle">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=signinMethod = "google"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_LinkedIn" name="LinkedIn" sourceRef="Gateway_SigninMethod" targetRef="Task_SigninLinkedIn">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=signinMethod = "linkedin"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_InvalidMethod" name="Invalid Method" sourceRef="Gateway_SigninMethod" targetRef="Task_HandleInvalidMethod">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=signinMethod != "google" and signinMethod != "linkedin"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_SigninGoogle" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_SigninLinkedIn" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Gateway_Join" targetRef="EndEvent_1" />
    <bpmn:sequenceFlow id="Flow_CaptchaError" sourceRef="BoundaryEvent_CaptchaError" targetRef="Task_HandleCaptchaError" />
    <bpmn:sequenceFlow id="Flow_CaptchaErrorEnd" sourceRef="Task_HandleCaptchaError" targetRef="EndEvent_CaptchaError" />
    <bpmn:sequenceFlow id="Flow_GoogleError" sourceRef="BoundaryEvent_GoogleError" targetRef="Task_HandleOAuthError" />
    <bpmn:sequenceFlow id="Flow_LinkedInError" sourceRef="BoundaryEvent_LinkedInError" targetRef="Task_HandleOAuthError" />
    <bpmn:sequenceFlow id="Flow_OAuthErrorEnd" sourceRef="Task_HandleOAuthError" targetRef="EndEvent_OAuthError" />
    <bpmn:sequenceFlow id="Flow_InvalidMethodEnd" sourceRef="Task_HandleInvalidMethod" targetRef="EndEvent_InvalidMethod" />
  </bpmn:process>
  <bpmn:error id="Error_CaptchaFailed" name="CAPTCHA_FAILED" errorCode="CAPTCHA_VALIDATION_FAILED" />
  <bpmn:error id="Error_OAuthFailed" name="OAUTH_FAILED" errorCode="OAUTH_AUTHENTICATION_FAILED" />
  <bpmn:error id="Error_InvalidMethod" name="INVALID_METHOD" errorCode="INVALID_SIGNIN_METHOD" />
  <bpmn:error id="Error_NetworkTimeout" name="NETWORK_TIMEOUT" errorCode="NETWORK_TIMEOUT_ERROR" />
  <bpmn:error id="Error_ServiceUnavailable" name="SERVICE_UNAVAILABLE" errorCode="EXTERNAL_SERVICE_UNAVAILABLE" />
