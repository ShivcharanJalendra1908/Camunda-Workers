        <di:waypoint x="540" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_UpdateError_di" bpmnElement="Flow_UpdateError">
        <di:waypoint x="440" y="178" />
        <di:waypoint x="440" y="220" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_EndUpdateError_di" bpmnElement="Flow_EndUpdateError">
        <di:waypoint x="440" y="300" />
        <di:waypoint x="440" y="342" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_DuplicateEmail_di" bpmnElement="Flow_DuplicateEmail">
        <di:waypoint x="490" y="178" />
        <di:waypoint x="490" y="200" />
        <di:waypoint x="590" y="200" />
        <di:waypoint x="590" y="220" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_EndDuplicateEmail_di" bpmnElement="Flow_EndDuplicateEmail">
        <di:waypoint x="590" y="300" />
        <di:waypoint x="590" y="342" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_UpdateTimeout_di" bpmnElement="Flow_UpdateTimeout">
        <di:waypoint x="440" y="62" />
        <di:waypoint x="440" y="60" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_EndTimeout_di" bpmnElement="Flow_EndTimeout">
        <di:waypoint x="440" y="-20" />
        <di:waypoint x="440" y="-84" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_CRMSynced_di" bpmnElement="Flow_CRMSynced">
        <di:waypoint x="640" y="120" />
        <di:waypoint x="690" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_CRMSyncError_di" bpmnElement="Flow_CRMSyncError">
        <di:waypoint x="590" y="178" />
        <di:waypoint x="590" y="200" />
        <di:waypoint x="740" y="200" />
        <di:waypoint x="740" y="220" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_ContinueAfterCRMSyncError_di" bpmnElement="Flow_ContinueAfterCRMSyncError">
        <di:waypoint x="740" y="220" />
        <di:waypoint x="740" y="160" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_ToAudit_di" bpmnElement="Flow_ToAudit">
        <di:waypoint x="790" y="120" />
        <di:waypoint x="840" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_EmailError_di" bpmnElement="Flow_EmailError">
        <di:waypoint x="740" y="178" />
        <di:waypoint x="740" y="200" />
        <di:waypoint x="890" y="200" />
        <di:waypoint x="890" y="220" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_ContinueAfterEmailError_di" bpmnElement="Flow_ContinueAfterEmailError">
        <di:waypoint x="890" y="220" />
        <di:waypoint x="890" y="160" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_EndUpdate_di" bpmnElement="Flow_EndUpdate">
        <di:waypoint x="940" y="120" />
        <di:waypoint x="992" y="120" />
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>

  </bpmndi:BPMNDiagram>
  
</bpmn:definitions>